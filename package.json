{
  "name": "loopback-boilerplate",
  "version": "0.3.0",
  "main": "build/server/server",
  "scripts": {
    "build:clean": "rm -rf build",
    "build:restoreLPfiles": "cp common/models/*.json build/common/models/ && cp server/*.json build/server",
    "prebuild": "npm run build:clean",
    "build": "./node_modules/.bin/tsc",
    "postbuild": "npm run build:restoreLPfiles",
    "start.local.built": "./node_modules/.bin/nodemon --debug -r dotenv/config build/server/server dotenv_config_path=environments/.local",
    "start.local": "./node_modules/.bin/nodemon --debug -r dotenv/config . dotenv_config_path=environments/.local",
    "start": "node .",
    "eslint": "./node_modules/.bin/eslint .",
    "tslint": "./node_modules/.bin/tslint .",
    "lint": "npm run tslint && npm run eslint",
    "pretest": "npm run lint",
    "posttest": "./node_modules/.bin/nsp check",
    "mocha": "./node_modules/.bin/mocha -R spec 'test/{,**/}*.js' -r dotenv/config dotenv_config_path=./environments/.test",
    "mochadebug": "./node_modules/.bin/mocha --debug-brk -R spec 'test/{,**/}*.js' -r dotenv/config dotenv_config_path=./environments/.test",
    "test": "npm run mocha",
    "coverage": "istanbul cover _mocha -- -R spec 'test/{,**/}*.js'",
    "writeversion": "node lib/writeVersionFile/index.js"
  },
  "engines": {
    "node": "6.x.x"
  },
  "dependencies": {
    "bcrypt": "1.0.2",
    "bluebird": "3.4.7",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "debug": "2.6.0",
    "helmet": "3.4.0",
    "lodash": "4.17.4",
    "loopback": "3.2.1",
    "loopback-boot": "2.23.0",
    "loopback-component-explorer": "4.1.0",
    "loopback-component-model-diagram": "0.3.0",
    "loopback-connector-mysql": "3.0.0",
    "morgan": "1.7.0",
    "serve-favicon": "2.3.2",
    "strong-error-handler": "1.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "1.0.0",
    "@types/bluebird": "3.0.37",
    "@types/debug": "0.0.29",
    "@types/loopback": "3.1.0-next.1",
    "@types/loopback-boot": "2.23.0-next.0",
    "@types/node": "7.0.4",
    "@types/serve-favicon": "2.2.28",
    "chai": "3.5.0",
    "dotenv": "4.0.0",
    "eslint": "3.14.1",
    "eslint-config-airbnb-base": "11.0.1",
    "eslint-plugin-import": "2.2.0",
    "git-rev-sync": "1.8.0",
    "husky": "0.12.0",
    "istanbul": "0.4.5",
    "jsonfile": "2.4.0",
    "mocha": "3.2.0",
    "mysql2": "1.1.2",
    "nsp": "2.6.2",
    "supertest": "3.0.0",
    "ts-node": "2.0.0",
    "tslint": "4.4.2",
    "typescript": "2.1.5"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "description": "loopback-boilerplate"
}