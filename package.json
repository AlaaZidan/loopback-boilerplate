{
  "name": "loopback-boilerplate",
  "version": "0.3.0",
  "main": "server/server.js",
  "scripts": {
    "start.local": "node_modules/.bin/nodemon --debug -r dotenv/config . dotenv_config_path=environments/.dev",
    "start": "node .",
    "lint": "./node_modules/.bin/eslint .",
    "pretest": "npm run lint",
    "posttest": "./node_modules/.bin/nsp check",
    "mocha": "./node_modules/.bin/mocha -R spec 'test/{,**/}*.js' -r dotenv/config dotenv_config_path=./environments/.test",
    "mochadebug": "./node_modules/.bin/mocha --debug-brk -R spec 'test/{,**/}*.js' -r dotenv/config dotenv_config_path=./environments/.test",
    "test": "npm run mocha",
    "coverage": "istanbul cover _mocha -- -R spec 'test/{,**/}*.js'",
    "writeversion": "node lib/writeVersionFile/index.js"
  },
  "engines": {
    "node": "6.x.x"
  },
  "dependencies": {
    "bcrypt": "1.0.1",
    "bluebird": "3.4.6",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "debug": "2.4.1",
    "helmet": "3.1.0",
    "lodash": "4.17.2",
    "loopback": "3.1.x",
    "loopback-boot": "2.23.0",
    "loopback-component-explorer": "2.7.0",
    "loopback-component-model-diagram": "0.2.0",
    "loopback-connector-mysql": "git://github.com/strongloop/loopback-connector-mysql#master",
    "loopback-datasource-juggler": "3.1.0",
    "morgan": "1.7.0",
    "serve-favicon": "2.3.2",
    "strong-error-handler": "1.1.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "dotenv": "2.0.0",
    "eslint": "3.12.1",
    "eslint-config-airbnb-base": "11.0.0",
    "eslint-plugin-import": "2.2.0",
    "git-rev-sync": "1.8.0",
    "istanbul": "0.4.5",
    "jsonfile": "2.4.0",
    "mocha": "3.2.0",
    "mysql2": "1.1.2",
    "nsp": "2.6.2",
    "supertest": "2.0.1"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "description": "loopback-boilerplate"
}